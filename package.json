{
  "name": "intl-data",
  "version": "1.0.9",
  "description": "Tiny module to retrieve locale and currency data from native builtin Intl object without locale data transfer or additional locale files.",
  "author": {
    "name": "Özüm Eldoğan",
    "email": "ozum@ozum.net",
    "url": ""
  },
  "keywords": [
    "locale",
    "currency",
    "intl",
    "native"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "dist/cjs/intl-data.js",
  "types": "dist/intl-data.d.ts",
  "module": "dist/esm/intl-data.mjs",
  "umd:main": "dist/umd/intl-data.js",
  "files": [
    "dist",
    "!dist/**/@(*.spec|*.test)*",
    "!dist/**/__test__",
    "module-files",
    "@types"
  ],
  "homepage": "https://github.com/ozum/intl-data",
  "bugs": "https://github.com/ozum/intl-data/issues",
  "repository": "ozum/intl-data",
  "license": "MIT",
  "scripts": {
    "execute": "ts-node --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "watch": "ts-node-dev --no-notify --respawn --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "lint": "eslint --ignore-path .gitignore --cache --max-warnings 0 --ext js,jsx,ts,tsx,vue --fix .",
    "format": "prettier . --write",
    "release": "npm run readme && git pull && git add -A && git commit && git push --follow-tags",
    "test": "npm run not-sync && jest --coverage",
    "readme": "node module-files/scripts/tsmod.js readme",
    "build": "npm run not-sync && rollup -c rollup.config.js",
    "postinstall": "is-ci || (husky install && npm run not-sync)",
    "postpublish": "pinst --enable",
    "not-sync": "node module-files/scripts/tsmod.js not-sync dist,node_modules,coverage",
    "prepublishOnly": "pinst --disable",
    "yo:update": "yo tsmod:uninstall --no-install --force && yo 'tsmod' '--no-import-helpers' '--builder' 'rollup' '--main' 'intl-data'"
  },
  "devDependencies": {
    "lodash.camelcase": "^4.3.0",
    "walkdir": "^0.4.1",
    "not-sync": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "commitizen": "^4.2.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^5.0.6",
    "pinst": "^2.1.2",
    "is-ci": "^2.0.0",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.2.0",
    "lint-staged": "^10.5.3",
    "readmeasy": "^0.2.7",
    "concat-md": "^0.3.5",
    "typedoc": "^0.20.19",
    "typedoc-neo-theme": "^1.1.0",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-markdown": "^3.4.3",
    "@types/node": "^14.14.22",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "rollup": "^2.38.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0"
  }
}
